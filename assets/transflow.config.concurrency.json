{
  "project": "my-media-app",
  "region": "us-east-1",
  "s3": {
    "mode": "prefix",
    "uploadBucket": "my-media-uploads",
    "outputBucket": "my-media-outputs"
  },
  "ecrRepo": "my-media-app-worker",
  "lambdaPrefix": "media-worker-",
  "templatesDir": "./templates",
  "lambdaBuildContext": "./lambda-build",
  "redis": {
    "provider": "upstash",
    "restUrl": "https://your-redis.upstash.io",
    "token": "your-redis-token"
  },
  "lambda": {
    "memoryMb": 2048,
    "timeoutSec": 900,
    "architecture": "x86_64",
    "roleArn": "arn:aws:iam::123456789012:role/TransflowExecutionRole",
    "reservedConcurrency": 50,
    "maxBatchSize": 5
  },
  "sqs": {
    "enabled": true,
    "queueName": "my-media-app-processing.fifo",
    "visibilityTimeoutSec": 960,
    "maxReceiveCount": 3,
    "batchSize": 10
  },
  "dynamoDb": {
    "enabled": true,
    "tableName": "TransflowJobs"
  }
}

